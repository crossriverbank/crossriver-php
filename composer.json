{
  "name": "crossriverbank/crossriver-php",
  "version": "1.0.1",
  "description": "A PHP HTTP Client conforming to the HAL hypermedia type for the Crossriver processing API",
  "license": "Apache2",
  "authors": [
    {
      "name": "Cross River Bank",
      "email": "support@crossriverbank.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "finix/processing-php": ">=1.0.4"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0.*"
  },
  "autoload": {
    "psr-0": {
      "Crossriver\\": "./src"
    },
    "psr-4": {
      "Crossriver\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Crossriver\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable" : true
}
